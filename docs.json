{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "almond",
  "name": "aicofounder docs",
  "logo": {
    "light": "/logo-dark.svg",
    "dark": "/logo-light.svg",
    "href": "https://aicofounder.com"
  },
  "favicon": "/icon.svg",
  "colors": {
    "primary": "#b85c44",
    "light": "#c97a66",
    "dark": "#a45139"
  },
  "fonts": {
    "family": "Figtree"
  },
  "background": {
    "color": {
      "light": "#f2f1ec",
      "dark": "#333333"
    }
  },
  "navbar": {
    "links": [
      {
        "label": "App",
        "href": "https://aicofounder.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get started",
      "href": "https://aicofounder.com/signup"
    }
  },
  "navigation": {
    "anchors": [
      {
        "anchor": "Blog",
        "href": "https://aicofounder.com/blog",
        "icon": "newspaper"
      }
    ],
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Get started",
            "pages": ["index", "quickstart"]
          },
          {
            "group": "Your workspace",
            "pages": ["canvas", "documents", "notes", "sections"]
          },
          {
            "group": "Working with your AI",
            "pages": ["chat", "brainstorming", "ultraplan", "deep-research"]
          },
          {
            "group": "Building websites",
            "pages": ["websites", "publishing"]
          },
          {
            "group": "Marketing",
            "pages": ["content-calendar"]
          },
          {
            "group": "Sharing",
            "pages": ["collaboration", "export"]
          },
          {
            "group": "Account & billing",
            "pages": ["privacy-mode", "plans-and-credits", "settings"]
          }
        ]
      }
    ]
  },
  "appearance": {
    "default": "light",
    "strict": true
  },
  "seo": {
    "metatags": {
      "og:site_name": "aicofounder",
      "og:description": "Learn how to use aicofounder to research, plan, and build your product",
      "twitter:card": "summary_large_image",
      "article:author": "aicofounder",
      "article:published_time": "2026-02-06"
    }
  },
  "footer": {
    "socials": {}
  }
}
