{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "almond",
  "name": "aicofounder",
  "logo": {
    "light": "/logo-dark.svg",
    "dark": "/logo-light.svg",
    "href": "https://aicofounder.com"
  },
  "favicon": "/icon.svg",
  "colors": {
    "primary": "#b85c44",
    "light": "#c97a66",
    "dark": "#a45139"
  },
  "fonts": {
    "family": "Figtree"
  },
  "background": {
    "color": {
      "light": "#f2f1ec",
      "dark": "#333333"
    }
  },
  "navbar": {
    "links": [
      {
        "label": "App",
        "href": "https://aicofounder.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get started",
      "href": "https://aicofounder.com/signup"
    }
  },
  "navigation": {
    "anchors": [
      {
        "anchor": "Blog",
        "href": "https://aicofounder.com/blog",
        "icon": "newspaper"
      }
    ],
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Get started",
            "pages": ["index", "quickstart"]
          },
          {
            "group": "Your workspace",
            "pages": ["canvas", "documents", "notes", "sections"]
          },
          {
            "group": "Working with your AI",
            "pages": ["chat", "brainstorming", "ultraplan", "deep-research"]
          },
          {
            "group": "Websites",
            "pages": ["websites", "publishing"]
          },
          {
            "group": "Content calendar",
            "pages": ["content-calendar"]
          },
          {
            "group": "Collaboration & export",
            "pages": ["collaboration", "exporting"]
          },
          {
            "group": "Account & billing",
            "pages": ["privacy-mode", "credits", "plans", "settings"]
          }
        ]
      }
    ]
  },
  "appearance": {
    "default": "light",
    "strict": true
  },
  "footer": {
    "socials": {}
  }
}
